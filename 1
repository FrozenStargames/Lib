Tab.TextWrapped = true

UICorner_6.Name = name.."Corner"
UICorner_6.Parent = Tab
UICorner_6.CornerRadius = UDim.new(0,10)

Tab.InputBegan:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseButton1 then
for i,v in pairs(PrePage:GetChildren()) do
if v.ClassName == "Frame" then
v.Visible = false
end
end
Page.Visible = true
end
end)
local closee = false
local UIS = game:GetService("UserInputService")
local ButtonOpenImage = ImageButton
local ButtonExit = TextButton_5
local Trangle = ImageLabel
ButtonOpenImage.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		if TextLabel_3.TextTransparency == 1 then
			Tween(ButtonOpenImage,.5,UDim2.new(0,0,0,24)):Play()
			Tween(ButtonExit,0,UDim2.new(0,683,0,460)):Play()
		Tween(Addition,.4,UDim2.new(0,172,0,460)):Play()
		Tween(Frame_3,.4,UDim2.new(0,171,0,1)):Play()
		wait(.4)
		for i,v in pairs(Main:GetDescendants()) do
			if v.Parent ~= Frame2 and v.ClassName == "TextButton" then
				v.Active = false
				v.AutoButtonColor = false
			end
		end
			for i,Buttons in pairs(Frame2:GetChildren()) do
			if Buttons.ClassName == "TextButton" then
				Tween(Buttons,0,UDim2.new(0,145,0,28)):Play()
				Tween2(Buttons,.3,0):Play()
				Tween3(Buttons,.3,0):Play()
			end
		end
		Tween2(TextLabel_3,.3,0):Play()
		Tween4(Trangle,.3,0):Play()
		end
		end
end)
local ButtonExit = TextButton_5
local ButtonOpenImage = ImageButton
local Triangle = ImageLabel

ButtonExit.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		if TextLabel_3.TextTransparency == 0 then
		Tween(ButtonExit,0,UDim2.new(0,0,0,460)):Play()
		for i,Buttons in pairs(Frame2:GetChildren()) do
			if Buttons.ClassName == "TextButton" then
				Tween2(Buttons,.3,1):Play()
				Tween3(Buttons,.3,1):Play()
			end
		end
		Tween2(TextLabel_3,.2,1):Play()
		wait(.2)
		for i,v in pairs(Main:GetDescendants()) do
			if v.Parent ~= Frame2 and v.ClassName == "TextButton" then
				v.Active = true
				v.AutoButtonColor = true
			end
		end
		for i,Buttons in pairs(Frame2:GetChildren()) do
			if Buttons.ClassName == "TextButton" then
				Tween(Buttons,0,UDim2.new(0,0,0,28)):Play()
			end
		end
		Tween4(Triangle,.3,1):Play()
		wait(.3)
		Tween(Addition,.4,UDim2.new(0,0,0,460)):Play()
		Tween(Frame_3,.4,UDim2.new(0,0,0,1)):Play()
		wait(.4)
		Tween(ButtonOpenImage,.3,UDim2.new(0,31,0,24)):Play()
		end
	end
end)

local dragstart = nil
local dragging = nil
local delta = nil
local startpos = nil
local function upd(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			local position = UDim2.new(startpos.X.Scale,startpos.X.Offset+delta.X,startpos.Y.Scale,startpos.Y.Offset + delta.Y)
		game:GetService("TweenService"):Create(Main,TweenInfo.new(.15),{Position = position}):Play()
	end
	end
Drag.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragstart = input.Position
		startpos = Main.Position
		dragging = true
		input.Changed:Connect(function(input2)
			if input[input2] == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)
UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			if dragging == true then
			delta = input.Position - dragstart
			upd(input)
		end
	end
end)
local Used = false
local Gui = ScreenGuiLe45
Minimize.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		if Used == false then
		for i,v in pairs(Gui:GetDescendants()) do
			if v.ClassName == "TextButton" or v.ClassName == "TextLabel" then
				Tween2(v,1.5,1):Play()
				Tween3(v,1.5,1):Play()
			end
		end
		for i,v in pairs(Gui:GetDescendants()) do
			if v.ClassName == "ImageButton" or v.ClassName == "ImageLabel" then
				Tween3(v,1.5,1):Play()
				Tween4(v,1.5,1):Play()
			end
		end
		a.Completed:Wait()
		for i,v in pairs(Gui:GetDescendants()) do
			if v.ClassName == "Frame" then
				if v.Parent ~= Gui and v.Name ~= "Drag" and v.Name ~=  "Minimize" and v.Name ~=  "Exit" then
					Tween3(v,1.5,1):Play()
				end
			end
		end
		for i,v in pairs(Gui:GetDescendants()) do
			if v.ClassName == "Frame" then
				if v.Parent == Gui then
					Tween(v,1.5,UDim2.new(0,849,0,0)):Play()
				end
			end
			end
			Used = true
		else
			for i,v in pairs(Gui:GetDescendants()) do
				if v.ClassName == "Frame" then
					if v.Name ~= "Drag" and v.Parent ~= Drag and v.Name ~= "Frame2" and v.Name ~= "Slider" and v.Name ~= "ToggleAll" then
						Tween3(v,1.5,0):Play()
					end
				end
			end
			for i,v in pairs(Gui:GetDescendants()) do
				if v.ClassName == "Frame" then
					if v.Parent == Gui then
						Tween(v,1.5,UDim2.new(0,849,0,485)):Play()
					end
				end
			end
			a.Completed:Wait()
			for i,v in pairs(Gui:GetDescendants()) do
				if v.ClassName == "TextButton" and v.Parent ~= Gui.Main.Frame2 and v ~= TextButton_5 then
					Tween2(v,1.5,0):Play()
					Tween3(v,1.5,0):Play()
				end
			end
			for i,v in pairs(Gui:GetDescendants()) do
				if v.ClassName == "TextLabel" and v ~= TextLabel_3 then
					Tween2(v,1.5,0):Play()
				end
			end
			for i,v in pairs(Gui:GetDescendants()) do
				if v.ClassName == "ImageButton" or v.ClassName == "ImageLabel" and v.Parent ~= Gui.Main.Addition then
					Tween4(v,1.5,0):Play()
				end
			end
			Used = false
		end
	end
end)
local Gui = ScreenGuiLe45
Exit.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		for i,v in pairs(Gui:GetDescendants()) do
			if v.ClassName == "TextButton" or v.ClassName == "TextLabel" then
				Tween2(v,1.5,1):Play()
				Tween3(v,1.5,1):Play()
			end
		end
		for i,v in pairs(Gui:GetDescendants()) do
			if v.ClassName == "ImageButton" or v.ClassName == "ImageLabel" then
				Tween3(v,1.5,1):Play()
				Tween4(v,1.5,1):Play()
			end
		end
		a.Completed:Wait()
		for i,v in pairs(Gui:GetDescendants()) do 
			if v.ClassName == "Frame" then
				Tween3(v,1,1):Play()
			end
		end
			wait(3)
		Gui:Destroy()
	end
end)
local delta = nil
for i,Buttons in pairs(Frame2:GetChildren()) do
	if Buttons.ClassName == "TextButton" then
		Buttons.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				delta = Buttons.AbsolutePosition.Y+8 - Triangle.AbsolutePosition.Y
				Tween5(Triangle,.5,UDim2.new(Triangle.Position.X.Scale,Triangle.Position.X.Offset,Triangle.Position.Y.Scale,Triangle.Position.Y.Offset + delta)):Play()
			end
		end)
	end
	end
