print(1)
local a = nil
local function Tween(Object,time1,Size1)
	a = game:GetService("TweenService"):Create(Object,TweenInfo.new(time1),{Size = Size1})
	return a
end
local function Tween2(Object,time1,Size1)
	a = game:GetService("TweenService"):Create(Object,TweenInfo.new(time1),{TextTransparency = Size1})
	return a
end
local function Tween3(Object,time1,Size1)
	a = game:GetService("TweenService"):Create(Object,TweenInfo.new(time1),{BackgroundTransparency = Size1})
	return a
end
local function Tween4(Object,time1,Size1)
	a = game:GetService("TweenService"):Create(Object,TweenInfo.new(time1),{ImageTransparency = Size1})
	return a
end
local function Tween5(Object,time1,Size1)
	a = game:GetService("TweenService"):Create(Object,TweenInfo.new(time1),{Position = Size1})
	return a
end
local function Tween6(Object,time1,Size1)
	a = game:GetService("TweenService"):Create(Object,TweenInfo.new(time1),{TextColor3 = Size1})
	return a
end

for i,v11 in pairs(game.CoreGui:GetChildren()) do
if v11.Name == "ScreenGuiLe45" then
v11:Destroy()
end
end
