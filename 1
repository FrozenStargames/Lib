function libPage:addPage(name)
local Page = Instance.new("Frame")
local UICorner_5 = Instance.new("UICorner")
local UICorner_6 = Instance.new("UICorner")
local PageGrid = Instance.new("UIGridLayout")
local Tab = Instance.new("TextButton")
Page.Name = name.."Page"
Page.Parent = PrePage
Page.BackgroundColor3 = Color3.new(0.117647, 0.113725, 0.184314)
Page.BorderSizePixel = 0
Page.Position = UDim2.new(0.014117647, 0, 0.0236464236, 0)
Page.Size = UDim2.new(0, 827, 0, 440)
Page.Visible = false

PageGrid.Name = name.."Grid"
PageGrid.Parent = Page
PageGrid.HorizontalAlignment = Enum.HorizontalAlignment.Left
PageGrid.SortOrder = Enum.SortOrder.LayoutOrder
PageGrid.CellSize = UDim2.new(0, 270, 0, 5)
PageGrid.FillDirectionMaxCells = 3

UICorner_5.Name = name.."Corner"
UICorner_5.Parent = Page
UICorner_5.CornerRadius = UDim.new(0,8)

Tab.Name = name.."Button"
Tab.Parent = Frame2
Tab.BackgroundColor3 = Color3.new(0.184314, 0.180392, 0.278431)
Tab.BackgroundTransparency = 1
Tab.BorderColor3 = Color3.new(0.615686, 0, 0)
Tab.BorderSizePixel = 0
Tab.Position = UDim2.new(0, 0, 0, 33)
Tab.Size = UDim2.new(0, 0, 0, 28)
Tab.ZIndex = 5
Tab.Font = Enum.Font.TitilliumWeb
Tab.TextColor3 = Color3.new(1, 1, 1)
Tab.TextScaled = true
Tab.TextSize = 25
