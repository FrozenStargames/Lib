Frame_2.Parent = ToggleBack
Frame_2.BackgroundColor3 = Color3.new(1, 1, 1)
Frame_2.Position = UDim2.new(0, 0, 0.0439999998, -1)
Frame_2.Size = UDim2.new(0, 25, 0, 14)

Corner.Parent = Frame_2
Corner.CornerRadius = UDim.new(1,0)

UISizeConstraint_7.Parent = Frame_2
UISizeConstraint_7.MaxSize = Vector2.new(16, 16)
UISizeConstraint_7.MinSize = Vector2.new(16, 16)
local value = false
local callback = callback or function() end
		ToggleBack.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
					if value == false then
					Tween5(ToggleBack.Frame,.3,UDim2.new(ToggleBack.Frame.Position.X.Scale,20,ToggleBack.Frame.Position.Y.Scale,ToggleBack.Frame.Position.Y.Offset)):Play()
					Tween6(TextLabel_2,.3,Color3.fromRGB(10, 209, 239)):Play()
					value = true
					pcall(callback,value)
				else
					Tween5(ToggleBack.Frame,.3,UDim2.new(ToggleBack.Frame.Position.X.Scale,0,ToggleBack.Frame.Position.Y.Scale,ToggleBack.Frame.Position.Y.Offset)):Play()
					Tween6(TextLabel_2,.3,Color3.fromRGB(5, 93, 107)):Play()
					value = false
					pcall(callback,value)
				end
			end
	end)
end
