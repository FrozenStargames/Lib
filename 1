local Toggled = false
		Open.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				if Toggled == false then
					Tween2(Label,.4,1):Play()
					Tween(Down,.5,UDim2.new(0,266,0,200)):Play()
					Toggled = true
				else
					Tween2(Label,.4,1):Play()
					Tween(Down,.5,UDim2.new(0,266,0,20)):Play()
					Toggled = false
				end
			end
		end)
for i,v in pairs(table) do
print(v)
end
end
delay(1,function()
    for i,Sizer in pairs(PrePage:GetDescendants()) do
	if Sizer.ClassName == "UISizeConstraint" then
		if Sizer.Parent.Parent.Parent == PrePage then
			for i,v in pairs(Sizer.Parent:GetChildren()) do
				if not string.find(tostring(v.ClassName),"UI") then
					Sizer.MaxSize = Vector2.new(270,Sizer.MaxSize.Y + v:FindFirstChildOfClass("UISizeConstraint").MaxSize.Y+6)
					Sizer.MinSize = Vector2.new(270,Sizer.MinSize.Y + v:FindFirstChildOfClass("UISizeConstraint").MinSize.Y+6)
				end
			end
		end
	end
end
end)
PrePage:FindFirstChildOfClass("Frame").Visible = true
return libFill
end
return libSection
end
return libPage
end
return library
