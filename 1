SlidValue.Text = min
SlidValue.Font = Enum.Font.Roboto
SlidValue.TextColor3 = Color3.new(1, 1, 1)
SlidValue.TextSize = 15
SlidValue.TextTransparency = 1

UISizeConstraint_11.Parent = SlidValue

Info.Name = "Info"
Info.Parent = Slider
Info.AnchorPoint = Vector2.new(0, 1)
Info.BackgroundColor3 = Color3.new(0.172549, 0.168627, 0.27451)
Info.BorderSizePixel = 0
Info.Position = UDim2.new(0.00370370364, 0, 1, 0)
Info.Size = UDim2.new(0, 268, 0, 15)
Info.Font = Enum.Font.Roboto
Info.Text = "Walkspeed Enchancer"
Info.TextColor3 = Color3.new(1, 1, 1)
Info.TextSize = 15
Info.TextWrapped = true
local UIS = game:GetService("UserInputService")
local function help(res)
if res > max then
res = max
return res
else return res
end
end
local function upd(input,Slideer)
	pos = UIS:GetMouseLocation()-Slideer.Parent.Slid.AbsolutePosition
	local percentage = math.clamp(pos.X/Slideer.Parent.Slid.AbsoluteSize.X,0,1)
	Slideer.Position = UDim2.new(percentage,-3,0,-2)
	Slideer.Parent.SlidValue.Text = math.round(percentage*max)
	pcall(callback,math.round(percentage*max))
	end
		ButtonSlid.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				startpos = ButtonSlid.Position
				sliding = true
				good = ButtonSlid
				ButtonSlid.InputEnded:Connect(function(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						sliding = false
					end
				end)
			end
		end)
		Slider.MouseEnter:Connect(function()
			if Addition.Size ~= UDim2.new(0,172,0,460) then
			Tween(SlidValue,.06,UDim2.new(SlidValue.Size.X.Scale,SlidValue.Size.X.Offset,SlidValue.Size.Y.Scale,15)):Play()
			Tween(Slider.Info,.08,UDim2.new(0,268,0,0))
			Tween2(Info,.05,1):Play()
			a.Completed:Wait()
			Tween2(SlidValue,.02,0):Play()
			a.Completed:Wait()
			Slider.MouseLeave:Connect(function()
				Tween2(SlidValue,.02,1):Play()
				a.Completed:Wait()
				Tween(SlidValue,.06,UDim2.new(Slider.SlidValue.Size.X.Scale,Slider.SlidValue.Size.X.Offset,Slider.SlidValue.Size.Y.Scale,0)):Play()
				a.Completed:Wait()
				Tween(Info,.08,UDim2.new(0,268,0,15))
					Tween2(Info,.05,0):Play()
				end)
			end
		end)
		ButtonSlid.Changed:Connect(function(prop)
			if prop == "Position" then
			end
		end)
		UIS.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				if sliding == true then
					upd(input,good)
				end
			end
		end)
end
function libFill:addDropdown(text,callback,table)
local Dropdown = Instance.new("Frame")
